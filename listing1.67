function setup() {
  createCanvas(200, 200);
  background(255);
  smooth();
  fill(192);
  noStroke();
  rect(40,40,40,40);
  
  applyMatrix();
  //Move the origin to the pivot point
  translate(40,40);
  
  //Rotate on the pivot
  rotate(radians(45));
  
  //Draw the square
  fill(0);
  rect(0,0,40,40);
  resetMatrix();
}

